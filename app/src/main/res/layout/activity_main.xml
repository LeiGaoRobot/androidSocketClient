<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:context="com.example.hasee.myapplication.MainActivity">


    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etPort_1"
            android:hint="端口号"
            android:layout_below="@+id/textView" android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/etUrl_1"/>
    <TextView
            android:text="请输入IP地址"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_alignTop="@+id/tvTips"
            android:layout_toEndOf="@+id/etPort_3" android:id="@+id/textView"/>
    <TextView
            android:text="请输入端口号"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvTips"
            android:layout_marginTop="13dp"
            android:layout_alignParentTop="true" android:layout_alignParentStart="true"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etPort_3"
            android:hint="端口号"
            android:layout_below="@+id/etPort_2" android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/etUrl_2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etPort_2"
            android:hint="端口号"
            android:layout_below="@+id/etPort_1" android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/etUrl_2" android:text="6340"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etPort_5"
            android:hint="端口号"
            android:layout_below="@+id/etPort_4" android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/etUrl_5"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etUrl_2"
            android:hint="IP地址"
            android:layout_below="@+id/etUrl_1" android:layout_alignParentEnd="true" android:text="192.168.1.10"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etUrl_1"
            android:hint="IP地址"
            android:layout_below="@+id/textView" android:layout_alignParentEnd="true"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etUrl_3"
            android:hint="IP地址"
            android:layout_below="@+id/etUrl_2" android:layout_alignParentEnd="true"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etPort_4"
            android:hint="端口号"
            android:layout_below="@+id/etPort_3" android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/etUrl_4"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etUrl_4"
            android:hint="IP地址"
            android:layout_alignBaseline="@+id/etPort_4" android:layout_alignBottom="@+id/etPort_4"
            android:layout_alignParentEnd="true"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etUrl_5"
            android:hint="IP地址"
            android:layout_below="@+id/etUrl_4" android:layout_alignParentEnd="true"/>
    <Button
            android:text="开始连接"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btSend"
            android:layout_below="@+id/etPort_5" android:layout_alignParentStart="true"/>
    <TextView
            android:text="请求结果"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvResult"
            android:layout_marginRight="46dp"
            android:layout_below="@+id/btSend" android:layout_alignParentStart="true" android:layout_marginTop="19dp"/>
    <TextView
            android:text="new"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_above="@+id/textView"
            android:layout_toEndOf="@+id/btSend" android:id="@+id/textView2"/>
</RelativeLayout>
